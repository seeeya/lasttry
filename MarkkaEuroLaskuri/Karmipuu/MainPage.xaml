        <TextBlock x:Name="karmipiiri" HorizontalAlignment="Left" Height="59" Margin="226,699,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="196" FontSize="24" Foreground="White"/>
        <Button x:Name="button" Content="Laske" HorizontalAlignment="Left" Height="72" Margin="55,456,0,0" VerticalAlignment="Top" Width="327" FontSize="32" Foreground="White" BorderBrush="#FF005DFF" Click="button_Click"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Height="123" Margin="55,69,0,0" TextWrapping="Wrap" Text="Laske ikkunan ja lasin pinta-alat ja karmin piiri " VerticalAlignment="Top" Width="327" Foreground="#FFFBFBFB" FontSize="26.667"/>

