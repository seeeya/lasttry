        	<Run Text="piiri"/>
        </TextBlock>
        <TextBox x:Name="lKorkeus" HorizontalAlignment="Left" Margin="249,197,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="60" Width="173" FontSize="21.333" Foreground="White"/>
        <TextBox x:Name="lLeveys" HorizontalAlignment="Left" Margin="249,261,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="60" Width="173" FontSize="21.333" Foreground="White"/>
        <TextBox x:Name="kKorkeus" HorizontalAlignment="Left" Margin="249,323,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="58" Width="173" FontSize="21.333" Foreground="White"/>
